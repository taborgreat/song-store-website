extends layout

block content

  

  div(id="song-list-container")
    script(type='module' src='/javascripts/audioVisualizer.mjs')
    div(id="song-list")
      ul
      each song in song_list
        li 
          a(href=song.url) #{song.title}
          br
          a(href=song.artist.url) #{song.artist.username}
          br
          br
          hr



      else
        li There are no songs.

 
